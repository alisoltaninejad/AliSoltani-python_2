print('-------- Ali Soltani --------')

print('عملگرهای محاسباتی (Arithmetic Operators)')
a = 10
b = 3
print(a + b)  # 13
print(a - b)  # 7
print(a * b)  # 30
print(a / b)  #3.333
print(a // b)  # 3
print(a % b)  # 1
print(a ** b)  #  1000

print('عملگرهای جبری (Comparison Operators)')
x = 5
y = 8
print(x == y)  #  False
print(x != y)  # True
print(x > y)  # False
print(x < y)  # True
print(x >= y)  # False
print(x <= y)  # True

print('عملگرهای انتسابی (Assignment Operators)')
num = 10  
num += 5; print('+=:', num)    # 15
num -= 3; print('-=:', num)    # 12
num *= 2; print('*=:', num)    # 24
num /= 4; print('/=:', num)    # 6.0
num //= 2; print('//=:', num)  # 3.0
num %= 2; print('%=:', num)    # 1.0
num **= 3; print('**=:', num)  # 1.0

print('عملگرهای منطقی (Logical Operators)')
p = True
q = False
print('AND:', p and q)   # False
print('OR:', p or q)     # True
print('NOT:', not p)     # False
